                android:layout_height="0.6dp"
                android:background="#666666" />

            <TextView
                android:id="@+id/tv_CoordinatorLayoutViewPagerWebViewActivity"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="CoordinatorLayoutViewPagerWebViewActivity" />

            <View
                android:layout_width="match_parent"
                android:layout_height="0.6dp"
                android:background="#666666" />

            <TextView
                android:id="@+id/tv_CoordinatorLayoutListActivity"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="CoordinatorLayoutListActivity" />

            <View
                android:layout_width="match_parent"
                android:layout_height="0.6dp"
                android:background="#666666" />
        </LinearLayout>
    </ScrollView>

</RelativeLayout>
