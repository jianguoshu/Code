                android:layout_height="50dp"
                android:gravity="center"
                android:text="ANRActivity" />

            <View
                android:layout_width="match_parent"
                android:layout_height="0.6dp"
                android:background="#666666" />

            <TextView
                android:id="@+id/tv_hashbimap_test"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center"
