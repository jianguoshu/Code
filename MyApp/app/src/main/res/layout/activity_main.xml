                android:layout_height="50dp"
                android:gravity="center"
                android:text="XYActivity" />

            <View
                android:layout_width="match_parent"
                android:layout_height="0.6dp"
                android:background="#666666" />

            <TextView
                android:id="@+id/tv_btn_recycler"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="RecyclerViewActivity" />

            <View
                android:layout_width="match_parent"
                android:layout_height="0.6dp"
                android:background="#666666" />

            <TextView
                android:id="@+id/tv_btn_toast"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="ToastActivity" />

            <View
                android:layout_width="match_parent"
                android:layout_height="0.6dp"
                android:background="#666666" />

            <TextView
                android:id="@+id/tv_btn_split_touch"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="SplitTouchActivity" />

            <View
                android:layout_width="match_parent"
                android:layout_height="0.6dp"
                android:background="#666666" />

            <TextView
                android:id="@+id/tv_btn_service_lifecycle"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="ServiceLifecycleActivity" />

            <View
                android:layout_width="match_parent"
                android:layout_height="0.6dp"
                android:background="#666666" />

            <TextView
                android:id="@+id/tv_btn_gridlayout"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="GridLayoutActivity" />

            <View
                android:layout_width="match_parent"
                android:layout_height="0.6dp"
                android:background="#666666" />

            <TextView
                android:id="@+id/tv_btn_over_alert"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center"
